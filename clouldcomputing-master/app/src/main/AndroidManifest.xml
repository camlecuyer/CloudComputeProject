<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sourcey.clouldcomputing" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/non_emergency"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sourcey.clouldcomputing.MainActivity"
            android:label="@string/non_emergency" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sourcey.clouldcomputing.LoginActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.sourcey.clouldcomputing.SignupActivity" android:theme="@style/AppTheme.Dark" />
    </application>

</manifest>
